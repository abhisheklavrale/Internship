   "source": [
    "# program to check whether a given string is palindrome or not \n",
    "\n",
    "my_str=\"1001\"\n",
    "rev_str=reversed(my_str)\n",
    "if list(my_str)==list(rev_str):\n",
    "    print(\"string is palindrome\")\n",
    "else:\n",
    "    print(\"string is not palindrome\")"
   ]
